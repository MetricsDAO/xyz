{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "generate:contracts": "node bin/get-contracts && npm run ethers-v5-bounty-question",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "postinstall": "remix setup node",
    "ethers-v5-bounty-question": "abi-types-generator './contracts/BountyQuestion.json' --provider=ethers_v5 --output='./app/types/generated' --name=BountyQuestion"
  },
  "dependencies": {
    "@carbon/icons-react": "^10.44.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@headlessui/react": "^1.6.4",
    "@remix-run/react": "^1.3.1",
    "@remix-run/vercel": "^1.3.1",
    "algoliasearch": "^4.13.0",
    "core-evm-contracts": "^1.1.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.8",
    "ipfs-http-client": "^56.0.1",
    "react": "^17.0.2",
    "react-csv": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-instantsearch-dom": "^6.23.4",
    "react-paginate": "^8.1.3",
    "remix": "^1.3.1",
    "remix-utils": "^3.2.0",
    "wagmi": "^0.4.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.1",
    "@remix-run/eslint-config": "^1.3.1",
    "@remix-run/serve": "^1.3.1",
    "@types/carbon__icons-react": "^10.31.2",
    "@types/react": "^17.0.24",
    "@types/react-csv": "^1.1.3",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.5",
    "concurrently": "^7.1.0",
    "eslint": "^8.11.0",
    "ethereum-abi-types-generator": "^1.3.2",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "react-query": "4.0.0-beta.23"
  },
  "engines": {
    "node": ">=15"
  }
}
