{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn run lint && yarn run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint '**/*.{ts,tsx}'",
    "dev": "concurrently \"yarn run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "prod": "yarn run build && remix-serve api",
    "postinstall": "remix setup node",
    "ladle:dev": "concurrently \"yarn run dev:css\" \"ladle serve --stories=ladle\""
  },
  "dependencies": {
    "@carbon/icons-react": "^10.44.0",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@faker-js/faker": "^7.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^2.0.12",
    "@mantine/core": "^5.6.2",
    "@mantine/dates": "^5.6.2",
    "@mantine/hooks": "^5.6.2",
    "@mantine/remix": "^5.6.2",
    "@prisma/client": "4.5.0",
    "@radix-ui/react-dropdown-menu": "^2.0.1",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@rainbow-me/rainbowkit": "^0.7.1",
    "@remix-run/react": "^1.7.0",
    "@remix-run/vercel": "^1.7.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/react-query": "^4.12.0",
    "algoliasearch": "^4.13.0",
    "aos": "^2.3.4",
    "buffer-polyfill": "npm:buffer@^6.0.3",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-instantsearch-dom": "^6.23.4",
    "remix": "^1.7.0",
    "remix-typedjson": "^0.1.3",
    "remix-utils": "^3.2.0",
    "wagmi": "^0.6.5"
  },
  "devDependencies": {
    "@ladle/react": "^2.4.4",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@types/aos": "^3.0.4",
    "@types/carbon__icons-react": "^10.31.2",
    "@types/react": "^17.0.49",
    "@types/react-dom": "^17.0.9",
    "@types/react-instantsearch-dom": "6.12.3",
    "autoprefixer": "^10.4.5",
    "concurrently": "^7.1.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.12",
    "prisma": "^4.5.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=15"
  }
